<?xml version="1.0" encoding="utf-8"?><merge xmlns:android="http://schemas.android.com/apk/res/android">    <LinearLayout        android:id="@+id/play_container"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="@color/room_new_skin_bg"        android:orientation="vertical">        <RelativeLayout            android:id="@+id/ppt_Layout"            android:layout_width="match_parent"            android:layout_height="200dp"            android:layout_gravity="center">            <FrameLayout                android:id="@+id/ppt_container"                android:layout_width="match_parent"                android:layout_height="match_parent"                android:layout_centerVertical="true" />            <FrameLayout                android:id="@+id/desktop_video_container"                android:layout_width="match_parent"                android:layout_height="match_parent"                android:layout_alignParentLeft="true"                android:layout_alignParentStart="true"                android:layout_alignParentTop="true" />            <include layout="@layout/menu_top_layout" />            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_alignParentBottom="true"                android:orientation="vertical">                <LinearLayout                    android:id="@+id/ll_trywatch"                    android:layout_width="wrap_content"                    android:layout_height="30dp"                    android:layout_above="@+id/operation_btn_container"                    android:layout_marginBottom="6dp"                    android:layout_marginLeft="10dp"                    android:background="@drawable/shape_gray_30"                    android:gravity="center"                    android:orientation="horizontal"                    android:paddingLeft="10dp"                    android:visibility="gone"                    android:paddingRight="10dp">                    <ImageView                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:src="@drawable/ic_zz_try_see_head" />                    <TextView                        android:id="@+id/tv_trywatch"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:layout_marginLeft="10dp"                        android:text="可试看90s  开通周周会员免费看"                        android:textColor="@color/white"                        android:textSize="10sp" />                </LinearLayout>                <include layout="@layout/playback_menu_bottom_layout" />            </LinearLayout>            <com.zhangbin.paint.view.InputBarView                android:id="@+id/inputEdt_layout"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:visibility="gone" />            <RelativeLayout                android:id="@+id/rl_vip_open"                android:layout_width="match_parent"                android:layout_height="match_parent"                android:visibility="gone"                android:background="@color/black">                <ImageView                    android:id="@+id/iv_vip_close"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:src="@drawable/go_back_icon" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_above="@+id/tv_vip_open"                    android:layout_centerHorizontal="true"                    android:layout_marginBottom="10dp"                    android:text="开通班海VIP会员后观看完整版本"                    android:textColor="@color/white"                    android:textSize="14sp" />                <TextView                    android:id="@+id/tv_vip_open"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_centerInParent="true"                    android:background="@drawable/ic_zz_try_see_btn"                    android:gravity="center"                    android:text="开通会员"                    android:textColor="@color/white" />            </RelativeLayout>        </RelativeLayout>        <!--<com.sanhai.psdapp.common.third.ht.view.PlaybackMessageView            android:id="@+id/tab_container"            android:layout_width="match_parent"            android:layout_height="0dp"            android:layout_weight="1"            android:orientation="vertical" />-->    </LinearLayout>    <FrameLayout        android:id="@+id/video_container"        android:layout_width="160dp"        android:layout_height="120dp"        android:visibility="gone"/></merge>